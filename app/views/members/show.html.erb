<%= render  "layouts/header" %>
<div class="container">
    <h3>
        Member Data: <%= @member.name %>
    </h3>
    <div class="col col-sm-12 col-md-12 col-lg-12 col-xl-12">
        <div class="col col-sm-4 col-md-4 col-lg-4 col-xl-4">
            Date of Birth: 
        </div>
        <div class="col col-sm-8 col-md-8 col-lg-8 col-xl-8">
            <%= @member.dob %>
        </div>
    </div>
    <div class="col col-sm-12 col-md-12 col-lg-12 col-xl-12">
        <div class="col col-sm-4 col-md-4 col-lg-4 col-xl-4">
            Address: 
        </div>
        <div class="col col-sm-8 col-md-8 col-lg-8 col-xl-8">
            <%= @member.address %>
        </div>
    </div>
</div>